{
  "name": "msw-server-helper",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@7.20.0",
  "description": "Dynamic modification of node mock service worker mocks from within browser-based tests like Cypress and Playwright",
  "main": "index.js",
  "scripts": {
    "prepare:pr": "pnpm changeset version && pnpm i",
    "prepare:pr:changeset": "pnpm changeset",
    "release:dry-run": "pnpm -r publish --dry-run",
    "release": "pnpm -r publish --access public",
    "cypress:dev": "pnpm --filter remix-example dev",
    "test:cypress": "pnpm --filter remix-example test:e2e:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Gregory Beaver <greg.beaver.cello@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "typescript": "4.9.4"
  }
}
